{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\backend\\index.js"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: start backend",
            "request": "launch",
            "command": "npm run start",
            "cwd": "${workspaceFolder}\\backend"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: start front end",
            "request": "launch",
            "command": "npm run start",
            "cwd": "${workspaceFolder}\\frontend"
        }
    ]
}